<!DOCTYPE html>
<html>
  <%- include('./partials/head', {title}) %>
  <body class="grey lighten-4">
    <div class="row">
      <div class="col s12">
        <h1><%= title %></h1>
      </div>
    </div>

    <div class="row">
      <div class="col s12">
        <h2>Items</h2>
      </div>

      <div class="col s12">
        <ul>
          <% items.forEach(item => { %>
          <li><%- include('./partials/item-card',{item}) %></li>
          <% }) %>
        </ul>
      </div>
    </div>
  </body>
</html>
