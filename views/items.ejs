<!DOCTYPE html>
<html>
  <%- include('./partials/head', {title}) %>
  <body class="grey lighten-4">
    <%- include('./partials/header',{title}) %>

    <main>
      <div class="container">
        <%- include('./partials/sidenav') %>
        <div class="row center-align">
          <div class="col s12">
            <h2 style="font-size: 2rem">Items</h2>
          </div>
        </div>

        <div class="row">
          <div class="col s12">
            <ul>
              <% items.forEach(item => { %>
              <li><%- include('./partials/item-card', {item}) %></li>
              <% }) %>
            </ul>
          </div>
        </div>
      </div>
    </main>
  </body>
</html>
